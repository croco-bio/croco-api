util.DatabaseConnection.dbdriver=com.mysql.jdbc.Driver

util.DatabaseConnection.dbconstr=jdbc:mysql://mysql:3306/regulatory

util.DatabaseConnection.user=regulatory
util.DatabaseConnection.password=regulatory

util.DatabaseConnection.autoReconnect=true

